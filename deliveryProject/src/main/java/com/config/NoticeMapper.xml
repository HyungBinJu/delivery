<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="NoticeMapper">
<select id="noticeList" resultType="Notice">
select notice_number, notice_title,notice_contents,notice_writer,notice_date from
notice_list 
</select>
<insert id="noticeInsert" parameterType="Notice">
insert into notice_list values(nl_notice_number_seq.nextval, #{notice_title},
#{notice_contents},#{notice_writer},sysdate)
</insert>
	

</mapper>